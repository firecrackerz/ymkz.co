{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "member-access": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "prettier": true
  },
  "linterOptions": {
    "exclude": ["**/*.json"]
  }
}
