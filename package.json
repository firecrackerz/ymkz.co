  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/ymkz.co"
  },
  "bugs": {
    "url": "https://github.com/ymkz/ymkz.co/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit $GIT_PARAMS",
      "pre-commit": "yarn lazy"
    }
  }
