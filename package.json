    "react-dom": "16.7.0",
    "react-navi": "0.10.13",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@commitlint/cli": "7.4.0",
    "@commitlint/config-conventional": "7.3.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.20",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "@types/yup": "0.26.7",
    "codecov": "3.1.0",
    "husky": "1.3.1",
    "navi-scripts": "0.10.13",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.3",
    "react-scripts": "2.1.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.2.4"
  },
  "author": {
    "name": "ymkz",
    "email": "ymkzly@gmail.com",
