{
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "javascript.format.enable": false,
  "markdown.preview.breaks": true,
  "path-autocomplete.pathMappings": {
    "src": "${folder}/src"
  }
}
